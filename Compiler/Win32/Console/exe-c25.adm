/// ヒープ

_Finish _finish;

struct Test
{
	var a = 1;
	
	function Show()
	{
		printfln("%p [%s] a = %d", this, __FUNCTION__, a);
	}
}

Test test1;
printfln("test1 = %p", test1);
test1.Show();

var test2 = (Test)malloc(sizeof Test);
test2.izer();
test2.ctor();
printfln("test2 = %p", test2);
test2.Show();
test2.a = 2;
test2.Show();
test2.dtor();
free(test2);
