<title>無名関数</title>

<include src="finish.xml" />

<?llp
var f = function(a, b) { return a - b; };
printfln("f(5, 3) = %d", f(5, 3));

wrapper(function()
{
  printfln("anonymous!");
});

function wrapper(ptr)
{
  ptr();
}
?>

