<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.10.20080211">

<!-- struct member function -->

<include src="finish.xml" />

<?llp
struct Point
{
  var x, y;

  function Show()
  {
    printfln("[%s] %d, %d", __FUNCTION__, x, y);
  }
}

// instance
Point p = { 2, 3 };
p.Show();

// pointer
//var pp : Point = p;
var pp = p; // 型推論
pp.Show();
?>

