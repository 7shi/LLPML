<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.9.20071021">

<!-- function returns value -->

<include src="finish.xml" />

<var-declare name="a">3</var-declare>
<var-declare name="b">4</var-declare>

printfln("%d + %d = %d", a, b, add(a, b));

<function name="add">
	<arg name="a" />
	<arg name="b" />
	<return>
		a + b
	</return>
</function>

</llpml>
