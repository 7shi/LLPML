<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.13.20080327">

<title>動的配列</title>

<include src="finish.xml" />
<include src="ArrayList.xml" />

<?llp
ArrayList arr;

arr.Add("abc");
arr.Add("def");
arr.Add("ghi");
arr.Add("jkl");
arr.ForEachWithOrder(function(obj, i)
{
  printfln("%d: %s", i, obj);
});
println();

printfln("RemoveAt(1);");
arr.RemoveAt(1);
arr.ForEachWithOrder(function(obj, i)
{
  printfln("%d: %s", i, obj);
});
println();

printfln("Remove(\"ghi\");");
arr.Remove("ghi");
arr.ForEachWithOrder(function(obj, i)
{
  printfln("%d: %s", i, obj);
});
?>

</llpml>
