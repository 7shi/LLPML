<llpml version="0.14.20080402">

<title>メンバの構造体</title>

<include src="finish.xml" />

<?llp
struct Point { var x, y; }
struct Points { Point p1, p2; }

Points p = { { 1, 2 }, { 3, 4 } };
vprintfln("p = ((%d, %d), (%d, %d))", p);

p.p1.x = 9;
p.p2.y = 8;
vprintfln("p = ((%d, %d), (%d, %d))", p);

struct Test1 { Test2 a; }
struct Test2 { var b; }
Test1 t1 = { { "Member Test" } };
t1.a.b.printfln();

struct Test3 { var a : Test2; }
Test3 t3 = { t1.a };
t3.a.b.printfln();
?>

</llpml>
