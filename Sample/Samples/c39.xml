<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.18.20080421">

<title>カリー化</title>

<include src="finish.xml" />
<include src="jit.xml" />

<?llp
var mul = \(a, b) => a * b;
var mul2 = \a => mul(2, a);
printfln("mul2(3) = %d", mul2(3));

var curried = \a => \b => a * b;
printfln("typeof curried = %s", typeof curried);
println();

var curried3 = curried(3);
printfln("typeof curried3 = %s", typeof curried3);
printfln("curried3(2) = %d", curried3(2));
println();

printfln("curried(3)(2) = %d", curried(3)(2));
?>

</llpml>
