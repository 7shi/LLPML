<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.11.20080323">

<!-- string functions -->

<?llp
function memcpy(dest : byte[], src : byte[], count)
{
  __memcpy(dest, src, count);
  return dest;
}

function memset(dest : byte[], c, count)
{
  __stosb(dest, c, count);
  return dest;
}
?>

</llpml>
