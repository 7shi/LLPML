<llpml version="0.11.20080323">

<!-- string functions -->

<?llp
function memcpy(dest, src, count)
{
  __memcpy(dest, src, count);
  return dest;
}

function memset(dest, c, count)
{
  __stosb(dest, c, count);
  return dest;
}
?>

