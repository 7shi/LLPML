<?xml version="1.0" encoding="utf-8" ?>
<llpml version="0.11.20080306">

<!-- string functions -->

<?llp
function memset(dest : byte[], c, count)
{
  var end = dest + count;
  if (dest < end)
  {
    for (var p = dest; p < end; p++)
      p[0] = c;
  }
  return dest;
}
?>

</llpml>
